{
    "name": "@swissgeo/components",
    "version": "1.0.0",
    "description": "NuxtUI module that contains SwissGeo components",
    "repository": "geoadmin/web-mapviewer",
    "license": "BSD-3-Clause",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/types.d.mts",
            "import": "./dist/module.mjs"
        }
    },
    "main": "./dist/module.mjs",
    "typesVersions": {
        "*": {
            ".": [
                "./dist/types.d.mts"
            ]
        }
    },
    "files": [
        "dist"
    ],
    "workspaces": [
        "playground"
    ],
    "scripts": {
        "dev": "pnpm run dev:prepare && nuxt dev playground",
        "dev:build": "nuxt build playground",
        "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground",
        "lint": "eslint .",
        "prepack": "nuxt-module-build build",
        "prepare": "nuxt-module-build build --stub && nuxt-module-build prepare  && pnpm run prepack",
        "release": "pnpm run lint && pnpm run test && pnpm run prepack && changelogen --release && npm publish && git push --follow-tags",
        "test": "vitest run",
        "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
        "test:watch": "vitest watch"
    },
    "dependencies": {
        "@nuxt/kit": "catalog:",
        "@swissgeo/theme": "workspace:"
    },
    "devDependencies": {
        "@nuxt/devtools": "catalog:",
        "@nuxt/eslint-config": "catalog:",
        "@nuxt/icon": "catalog:",
        "@nuxt/module-builder": "catalog:",
        "@nuxt/schema": "catalog:",
        "@nuxt/test-utils": "catalog:",
        "@nuxt/ui": "catalog:",
        "@nuxtjs/i18n": "catalog:",
        "@swissgeo/config-eslint": "workspace:",
        "@types/node": "catalog:",
        "changelogen": "catalog:",
        "eslint": "catalog:",
        "nuxt": "catalog:",
        "tailwindcss": "catalog:",
        "typescript": "catalog:",
        "vitest": "catalog:",
        "vue-tsc": "catalog:"
    }
}
