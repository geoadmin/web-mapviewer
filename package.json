{
    "name": "web-mapviewer",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "npm run dev",
        "dev": "vite --port 8080",
        "preview": "vite build && vite preview --port 8080",
        "test:unit": "vitest --run --environment jsdom",
        "test:e2e": "start-server-and-test preview 8080 'cypress open'",
        "test:e2e:tablet": "start-server-and-test preview 8080 'cypress open --config viewportWidth=768,viewportHeight=1024'",
        "test:e2e:desktop": "start-server-and-test preview 8080 'cypress open --config viewportWidth=1440,viewportHeight=900'",
        "test:e2e:ci": "start-server-and-test preview 8080 'cypress run'",
        "test:ci": "npm run test:unit && npm run test:e2e:ci",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "build": "vite build",
        "deploy:dev": "DEPLOY=true npm run build -- --mode development && node scripts/deploy.js dev",
        "deploy:int": "DEPLOY=true npm run build -- --mode development && node scripts/deploy.js int",
        "update:translations": "node scripts/generate-i18n-files.js",
        "update:browserlist": "npx browserslist@latest --update-db"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.1.1",
        "@fortawesome/free-brands-svg-icons": "^6.1.1",
        "@fortawesome/free-regular-svg-icons": "^6.1.1",
        "@fortawesome/free-solid-svg-icons": "^6.1.1",
        "@fortawesome/vue-fontawesome": "^3.0.0-5",
        "@ivanv/vue-collapse-transition": "^1.0.2",
        "@popperjs/core": "^2.11.5",
        "animate.css": "^4.1.1",
        "axios": "^0.27.2",
        "bootstrap": "^5.1.3",
        "d3": "^7.4.4",
        "file-saver": "^2.0.5",
        "form-data": "^4.0.0",
        "hammerjs": "^2.0.8",
        "jquery": "^3.6.0",
        "ol": "^6.14.1",
        "pako": "^2.0.4",
        "print-js": "^1.6.0",
        "proj4": "^2.8.0",
        "register-service-worker": "^1.7.2",
        "reproject": "^1.2.6",
        "tippy.js": "^6.3.7",
        "vue": "^3.2.37",
        "vue-i18n": "^9.1.10",
        "vue-router": "^4.0.15",
        "vue-social-sharing": "next",
        "vuex": "^4.0.2"
    },
    "devDependencies": {
        "@cypress/vite-dev-server": "^2.2.3",
        "@cypress/vue": "^3.1.2",
        "@nuintun/qrcode": "^3.3.0",
        "@rushstack/eslint-patch": "^1.1.3",
        "@vitejs/plugin-vue": "^2.3.3",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^10.0.0",
        "aws-sdk": "^2.1150.0",
        "caniuse-lite": "^1.0.30001350",
        "chai": "^4.3.6",
        "cypress": "^10.0.3",
        "cypress-browser-permissions": "^1.1.0",
        "cypress-recurse": "^1.20.0",
        "cypress-wait-until": "^1.7.2",
        "dotenv": "^16.0.1",
        "eslint": "^8.17.0",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-prettier-vue": "^3.1.0",
        "eslint-plugin-vue": "^9.1.0",
        "git-branch": "^2.0.1",
        "googleapis": "^101.0.0",
        "jsdom": "^19.0.0",
        "mime-types": "^2.1.35",
        "prettier": "^2.6.2",
        "prettier-plugin-jsdoc": "^0.3.38",
        "sass": "^1.52.2",
        "start-server-and-test": "^1.14.0",
        "vite": "^2.9.10",
        "vitest": "^0.14.1",
        "vue-loader": "^17.0.0",
        "yargs": "^17.5.1"
    },
    "engines": {
        "node": ">=14",
        "npm": "8.*.*"
    },
    "engineStrict": true
}
