{
    "name": "web-mapviewer",
    "private": true,
    "scripts": {
        "start": "npm run dev",
        "dev": "vite --port 8080 --host --cors",
        "preview": "npm run build:prod && vite preview --port 8080 --host --outDir dist/production",
        "preview:dev": "npm run build:dev && vite preview --port 8080 --host --outDir dist/development",
        "preview:int": "npm run build:int && vite preview --port 8080 --host --outDir dist/integration",
        "test:unit": "npm run delete:reports:unit && vitest --run --environment jsdom",
        "test:unit:watch": "npm run delete:reports:unit && vitest --environment jsdom",
        "test:e2e": "start-server-and-test dev 8080 'cypress open'",
        "test:e2e:tablet": "start-server-and-test preview 8080 'cypress open --config viewportWidth=768,viewportHeight=1024'",
        "test:e2e:desktop": "start-server-and-test preview 8080 'cypress open --config viewportWidth=1440,viewportHeight=900'",
        "test:e2e:ci": "npm run delete:reports:e2e && start-server-and-test preview 8080 'cypress run'",
        "lint": "eslint . --fix --ignore-path .gitignore --ext .js,.vue",
        "format": "prettier --write src --list-different && prettier --write tests --list-different",
        "ci-format-check": "prettier --check --log-level warn src && prettier --check --log-level warn tests",
        "format-lint": "npm run format && npm run lint",
        "type-check": "vue-tsc --noEmit -p tsconfig.json --composite false",
        "build": "npm run type-check && vite build",
        "build:dev": "npm run build -- --mode development",
        "build:int": "npm run build -- --mode integration",
        "build:prod": "npm run build -- --mode production",
        "update:translations": "node scripts/generate-i18n-files.js",
        "delete:reports": "rimraf tests/results/ || true",
        "delete:reports:unit": "rimraf tests/results/unit/ || true",
        "delete:reports:e2e": "rimraf tests/results/e2e/ || true"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.4.2",
        "@fortawesome/free-brands-svg-icons": "^6.4.2",
        "@fortawesome/free-regular-svg-icons": "^6.4.2",
        "@fortawesome/free-solid-svg-icons": "^6.4.2",
        "@fortawesome/vue-fontawesome": "^3.0.5",
        "@geoblocks/cesium-compass": "^0.5.0",
        "@geoblocks/ol-maplibre-layer": "^0.1.2",
        "@ivanv/vue-collapse-transition": "^1.0.2",
        "@mapbox/togeojson": "^0.16.2",
        "@popperjs/core": "^2.11.8",
        "@turf/distance": "^6.5.0",
        "@turf/helpers": "^6.5.0",
        "@turf/point-to-line-distance": "^6.5.0",
        "animate.css": "^4.1.1",
        "axios": "^1.6.2",
        "bootstrap": "^5.3.2",
        "cesium": "^1.111.0",
        "chart.js": "^4.4.0",
        "chartjs-plugin-zoom": "^2.0.1",
        "file-saver": "^2.0.5",
        "form-data": "^4.0.0",
        "geographiclib-geodesic": "^2.0.0",
        "hammerjs": "^2.0.8",
        "jquery": "^3.7.1",
        "liang-barsky": "^1.0.5",
        "maplibre-gl": "^3.6.1",
        "ol": "^8.2.0",
        "ol-cesium": "^2.16.0",
        "pako": "^2.1.0",
        "print-js": "^1.6.0",
        "proj4": "^2.9.2",
        "reproject": "^1.2.7",
        "tippy.js": "^6.3.7",
        "vue": "^3.3.8",
        "vue-chartjs": "^5.2.0",
        "vue-i18n": "^9.6.5",
        "vue-router": "^4.2.5",
        "vue-social-sharing": "next",
        "vuex": "^4.1.0"
    },
    "devDependencies": {
        "@cypress/vite-dev-server": "^5.0.6",
        "@cypress/vue": "^6.0.0",
        "@nuintun/qrcode": "^3.4.0",
        "@rushstack/eslint-patch": "^1.5.1",
        "@types/jsdom": "^21.1.5",
        "@types/node": "^18.18.9",
        "@vitejs/plugin-vue": "^4.5.0",
        "@vue/eslint-config-prettier": "^8.0.0",
        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/tsconfig": "^0.4.0",
        "axios-retry": "^3.9.0",
        "chai": "^4.3.10",
        "cypress": "^13.5.1",
        "cypress-browser-permissions": "^1.1.0",
        "cypress-multi-reporters": "^1.6.4",
        "cypress-real-events": "^1.11.0",
        "cypress-recurse": "^1.35.2",
        "cypress-vite": "^1.4.2",
        "cypress-wait-until": "^1.7.2",
        "dotenv": "^16.3.1",
        "eslint": "^8.53.0",
        "eslint-plugin-cypress": "^2.15.1",
        "eslint-plugin-markdownlint": "^0.5.0",
        "eslint-plugin-prettier-vue": "^5.0.0",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-vue": "^9.18.1",
        "git-describe": "^4.1.1",
        "googleapis": "^128.0.0",
        "jsdom": "^22.1.0",
        "mime-types": "^2.1.35",
        "mocha-junit-reporter": "^2.2.1",
        "prettier": "^3.1.0",
        "prettier-plugin-jsdoc": "^1.1.1",
        "rimraf": "^5.0.5",
        "sass": "^1.69.5",
        "start-server-and-test": "^1.11.0",
        "typescript": "^5.2.2",
        "vite": "^4.5.1",
        "vite-node": "^0.34.6",
        "vite-plugin-static-copy": "^0.17.0",
        "vitest": "^0.34.6",
        "vue-tsc": "^1.8.19",
        "write-yaml-file": "^5.0.0",
        "yargs": "^17.7.2"
    },
    "engines": {
        "node": ">=18",
        "npm": "9.*.*"
    },
    "type": "commonjs",
    "engineStrict": true
}
