{
    "name": "web-mapviewer",
    "private": true,
    "scripts": {
        "start": "npm run dev",
        "dev": "vite --port 8080 --host --cors",
        "preview": "npm run build:prod && vite preview --port 8080 --host --outDir dist/production",
        "preview:dev": "npm run build:dev && vite preview --port 8080 --host --outDir dist/development",
        "preview:int": "npm run build:int && vite preview --port 8080 --host --outDir dist/integration",
        "lint": "eslint . --fix --ignore-path .gitignore --ext .js,.vue",
        "lint:no-fix": "eslint . --ignore-path .gitignore --ext .js,.vue",
        "test:unit": "npm run delete:reports:unit && vitest --run --environment jsdom",
        "test:unit:watch": "npm run delete:reports:unit && vitest --environment jsdom",
        "test:e2e": "start-server-and-test dev 8080 'cypress open'",
        "test:e2e:headless": "start-server-and-test dev 8080 'cypress run'",
        "test:e2e:tablet": "start-server-and-test preview 8080 'cypress open --config viewportWidth=768,viewportHeight=1024'",
        "test:e2e:desktop": "start-server-and-test preview 8080 'cypress open --config viewportWidth=1440,viewportHeight=900'",
        "test:e2e:ci": "npx cypress run  --record --group e2e/electron/mobile --tag ${CYPRESS_TAGS} --parallel --ci-build-id ${CODEBUILD_INITIATOR}",
        "test:component": "cypress open --component",
        "test:component:ci": "cypress run --component --record --tag ${CYPRESS_TAGS} --group component/electron/mobile --ci-build-id ${CODEBUILD_INITIATOR}",
        "type-check": "vue-tsc --noEmit -p tsconfig.dom.json --composite false",
        "build": "npm run type-check && vite build",
        "build:dev": "npm run build -- --mode development",
        "build:int": "npm run build -- --mode integration",
        "build:prod": "npm run build -- --mode production",
        "update:translations": "node scripts/generate-i18n-files.js",
        "build:fontawesome-pro": "npm config set @fortawesome:registry https://npm.fontawesome.com/ && npm config set //npm.fontawesome.com/:_authToken ${FONTAWESOME_PRO_TOKEN}",
        "delete:reports": "rimraf tests/results/ || true",
        "delete:reports:unit": "rimraf tests/results/unit/ || true"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.5.1",
        "@fortawesome/free-brands-svg-icons": "^6.5.1",
        "@fortawesome/free-regular-svg-icons": "^6.5.1",
        "@fortawesome/free-solid-svg-icons": "^6.5.1",
        "@fortawesome/vue-fontawesome": "^3.0.5",
        "@geoblocks/cesium-compass": "^0.5.0",
        "@geoblocks/ol-maplibre-layer": "^0.1.2",
        "@ivanv/vue-collapse-transition": "^1.0.2",
        "@mapbox/togeojson": "^0.16.2",
        "@popperjs/core": "^2.11.8",
        "@turf/boolean-contains": "^6.5.0",
        "@turf/boolean-point-in-polygon": "^6.5.0",
        "@turf/centroid": "^6.5.0",
        "@turf/distance": "^6.5.0",
        "@turf/helpers": "^6.5.0",
        "@turf/point-to-line-distance": "^6.5.0",
        "animate.css": "^4.1.1",
        "axios": "^1.6.2",
        "bootstrap": "^5.3.2",
        "cesium": "^1.112.0",
        "chart.js": "^4.4.1",
        "chartjs-plugin-zoom": "^2.0.1",
        "file-saver": "^2.0.5",
        "form-data": "^4.0.0",
        "geographiclib-geodesic": "^2.0.0",
        "hammerjs": "^2.0.8",
        "jquery": "^3.7.1",
        "liang-barsky": "^1.0.5",
        "maplibre-gl": "^3.6.2",
        "ol": "^8.2.0",
        "ol-cesium": "^2.16.0",
        "pako": "^2.1.0",
        "print-js": "^1.6.0",
        "proj4": "^2.9.2",
        "reproject": "^1.2.7",
        "tippy.js": "^6.3.7",
        "vue": "^3.3.10",
        "vue-chartjs": "^5.2.0",
        "vue-i18n": "^9.8.0",
        "vue-router": "^4.2.5",
        "vue-social-sharing": "next",
        "vuex": "^4.1.0"
    },
    "optionalDependencies":{
        "@fortawesome/pro-regular-svg-icons": "^6.5.1"
    },
    "devDependencies": {
        "@cypress/vite-dev-server": "^5.0.6",
        "@cypress/vue": "^6.0.0",
        "@nuintun/qrcode": "^3.4.0",
        "@rushstack/eslint-patch": "^1.6.0",
        "@types/jsdom": "^21.1.6",
        "@types/node": "^18.19.2",
        "@vitejs/plugin-vue": "^4.5.1",
        "@vue/eslint-config-prettier": "^8.0.0",
        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/tsconfig": "^0.4.0",
        "axios-retry": "^4.0.0",
        "chai": "^4.3.10",
        "cypress": "^13.6.1",
        "cypress-browser-permissions": "^1.1.0",
        "cypress-real-events": "^1.11.0",
        "cypress-recurse": "^1.35.2",
        "cypress-vite": "^1.5.0",
        "cypress-wait-until": "^3.0.1",
        "dotenv": "^16.3.1",
        "eslint": "^8.55.0",
        "eslint-plugin-cypress": "^2.15.1",
        "eslint-plugin-markdownlint": "^0.5.0",
        "eslint-plugin-mocha": "^10.2.0",
        "eslint-plugin-prettier-vue": "^5.0.0",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-vue": "^9.19.2",
        "git-describe": "^4.1.1",
        "googleapis": "^129.0.0",
        "jsdom": "^23.0.1",
        "mime-types": "^2.1.35",
        "mocha-junit-reporter": "^2.2.1",
        "prettier": "^3.1.0",
        "prettier-plugin-jsdoc": "^1.1.1",
        "rimraf": "^5.0.5",
        "sass": "^1.69.5",
        "start-server-and-test": "^2.0.3",
        "typescript": "^5.3.2",
        "vite": "^5.0.5",
        "vite-node": "^1.0.1",
        "vite-plugin-static-copy": "^1.0.0",
        "vitest": "^1.0.1",
        "vue-tsc": "^1.8.25",
        "write-yaml-file": "^5.0.0",
        "yargs": "^17.7.2"
    },
    "engines": {
        "node": ">=18",
        "npm": "9.*.*"
    },
    "type": "module",
    "engineStrict": true
}
