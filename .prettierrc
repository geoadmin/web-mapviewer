{
    "printWidth": 100,
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5",
    "tabWidth": 4,
    "jsxSingleQuote": false,
    "plugins": ["prettier-plugin-jsdoc", "@prettier/plugin-xml", "prettier-plugin-packagejson"],
    "overrides": [
        {
            "files": "*.md",
            "options": {
                "tabWidth": 2
            }
        }
    ],
    "singleAttributePerLine": true
}
